{
  "name": "@asterkit/core",
  "version": "0.1.0",
  "description": "TypeScript SDK for Aster agent and builder management APIs.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "SKILL.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xcipher9/asterkit.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/0xcipher9/asterkit/issues"
  },
  "homepage": "https://github.com/0xcipher9/asterkit/tree/main/packages/core",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aster",
    "asterdex",
    "sdk",
    "agent",
    "builder",
    "viem"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:integration": "bun test src/*.integration.test.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "viem": "^2.46.1"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
